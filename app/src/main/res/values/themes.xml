<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Theme.SanFranciscoDentalClinic" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorPrimary">@color/dental_navy</item>
        <item name="colorPrimaryVariant">#141B52</item>
        <item name="colorOnPrimary">@color/white</item>

        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
    </style>

    <style name="TimeButtonStyle" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="android:textColor">@color/grey_text</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">14sp</item>
        <item name="strokeColor">#E0E0E0</item>
        <item name="strokeWidth">1dp</item>
        <item name="cornerRadius">8dp</item>
        <item name="backgroundTint">@color/white</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
    </style>

</resources>